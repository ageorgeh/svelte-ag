{
  "name": "svelte-ag",
  "description": "Useful svelte components",
  "version": "1.0.0",
  "author": "Alexander Hornung",
  "bugs": "https://github.com/ageorgeh/svelte-ag/issues",
  "dependencies": {
    "clsx": "2.1.1",
    "svelte-toolbelt": "0.9.3",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0",
    "dequal": "2.0.3",
    "@floating-ui/dom": "1.7.3",
    "@dnd-kit-svelte/core": "0.0.11",
    "@dnd-kit-svelte/sortable": "0.0.11",
    "@dnd-kit-svelte/utilities": "0.0.11",
    "embla-carousel-svelte": "8.6.0",
    "svader": "0.5.5",
    "runed": "0.31.1",
    "bits-ui": "2.9.4",
    "@rollup/pluginutils": "5.2.0",
    "sveltekit-superforms": "2.27.1",
    "formsnap": "2.0.1",
    "valibot": "1.1.0",
    "radash": "12.1.1",
    "devalue": "5.1.1",
    "bottleneck": "2.19.5",
    "ts-ag": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^5.28.2",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.2.9",
    "tailwind-variants": "^1.0.0",
    "@lucide/svelte": "^0.508.0"
  },
  "devDependencies": {
    "svelte": "5.38.2",
    "@sveltejs/package": "^2.5.0",
    "@sveltejs/kit": "2.36.1",
    "@sveltejs/vite-plugin-svelte": "6.1.3",
    "@eslint/js": "^9.33.0",
    "@iconify/types": "^2.0.0",
    "@iconify/tailwind4": "^1.0.6",
    "@iconify/json": "^2.2.376",
    "@lucide/svelte": "^0.540.0",
    "tailwindcss": "4.1.12",
    "tw-animate-css": "1.3.7",
    "@tailwindcss/vite": "4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-better-tailwindcss": "3.7.5",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "typescript-svelte-plugin": "0.3.50",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "npm-check-updates": "^18.0.2",
    "@types/node": "^24.3.0",
    "rollup": "^4.46.4"
  },
  "type": "module",
  "files": [
    "./src",
    "./dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./vite": {
      "types": "./dist/lib/vite/index.d.ts",
      "default": "./dist/lib/vite/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "svelte": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "repository": "ageorgeh/svelte-ag",
  "scripts": {
    "project:package": "svelte-package -i src",
    "project:watch": "svelte-package -i src -w",
    "svelte:sync": "svelte-kit sync",
    "publish:local": "pnpm version prerelease --preid dev --no-git-tag-version && pnpm publish --registry http://localhost:4873 --tag dev --access public --no-git-checks --json > ./publishLocal.json",
    "publish:prerelease": "pnpm publish --tag dev --access public --no-git-checks --registry=https://registry.npmjs.org/ --json > ./publish.json",
    "version:prerelease": "pnpm version prerelease --preid dev --no-git-tag-version",
    "run-publish": "pnpm publish --access public --no-git-checks --registry=https://registry.npmjs.org/ --json > ./publish.json",
    "run-version": "pnpm version --no-git-tag-version"
  }
}
