{
  "name": "svelte-ag",
  "version": "1.0.9",
  "description": "Useful svelte components",
  "bugs": "https://github.com/ageorgeh/svelte-ag/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ageorgeh/svelte-ag.git"
  },
  "author": "Alexander Hornung",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./vite": {
      "types": "./dist/lib/vite/index.d.ts",
      "default": "./dist/lib/vite/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "svelte": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "files": [
    "./src",
    "./dist"
  ],
  "scripts": {
    "build": "svelte-package -i src",
    "deps:bump": "pnpm dlx npm-check-updates -u --target minor -ws",
    "deps:refresh": "pnpm deps:bump && pnpm install",
    "deps:update": "pnpm -r update",
    "prepare": "husky",
    "publish:local": "pnpm publish --registry http://localhost:4873 --tag dev --access public --no-git-checks --json > ./publishLocal.json",
    "publish:remote": "pnpm publish --registry https://registry.npmjs.org/ --access public --no-git-checks --json > ./publish.json",
    "run-publish": "pnpm publish:local && pnpm publish:remote",
    "run-version": "pnpm version patch --no-git-tag-version",
    "svelte:sync": "svelte-kit sync",
    "version:prerelease": "pnpm version prerelease --preid dev --no-git-tag-version",
    "watch": "svelte-package -i src -w"
  },
  "dependencies": {
    "@dnd-kit-svelte/core": "0.0.11",
    "@dnd-kit-svelte/sortable": "0.0.11",
    "@dnd-kit-svelte/utilities": "0.0.11",
    "@floating-ui/dom": "1.7.4",
    "@rollup/pluginutils": "5.3.0",
    "bits-ui": "2.15.4",
    "bottleneck": "2.19.5",
    "clsx": "2.1.1",
    "dequal": "2.0.3",
    "devalue": "5.6.2",
    "embla-carousel-svelte": "8.6.0",
    "formsnap": "2.0.1",
    "radash": "12.1.1",
    "runed": "0.37.1",
    "svader": "0.5.5",
    "svelte-toolbelt": "0.10.6",
    "sveltekit-superforms": "2.29.1",
    "tailwind-merge": "3.4.0",
    "tailwind-variants": "2.1.0",
    "ts-ag": "1.0.5",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@actions/languageserver": "^0.3.44",
    "@eslint/js": "^9.39.2",
    "@iconify/json": "^2.2.431",
    "@iconify/tailwind4": "^1.2.1",
    "@iconify/types": "^2.0.0",
    "@lucide/svelte": "^0.562.0",
    "@sveltejs/kit": "2.50.0",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "^24.10.9",
    "@typescript/native-preview": "7.0.0-dev.20260122.2",
    "eslint": "^9.39.2",
    "eslint-plugin-better-tailwindcss": "3.8.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^18.3.1",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-svelte": "^3.4.1",
    "rollup": "^4.56.0",
    "semantic-release": "^25.0.3",
    "svelte": "5.48.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "typescript-svelte-plugin": "0.3.50",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@lucide/svelte": "^0.508.0",
    "svelte": "^5.28.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide"
    ]
  }
}
