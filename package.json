{
  "name": "svelte-ag",
  "description": "Useful svelte components",
  "version": "0.0.2-dev.83",
  "author": "Alexander Hornung",
  "bugs": "https://github.com/ageorgeh/svelte-ag/issues",
  "dependencies": {
    "clsx": "2.1.1",
    "svelte-toolbelt": "0.9.2",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "1.0.0",
    "dequal": "2.0.3",
    "@floating-ui/dom": "1.7.1",
    "@dnd-kit-svelte/core": "0.0.8",
    "@dnd-kit-svelte/sortable": "0.0.8",
    "@dnd-kit-svelte/utilities": "0.0.8",
    "embla-carousel-svelte": "8.6.0",
    "svader": "0.5.4",
    "runed": "0.29.0",
    "bits-ui": "2.8.8",
    "@rollup/pluginutils": "5.2.0",
    "sveltekit-superforms": "2.27.0",
    "formsnap": "2.0.1",
    "valibot": "1.1.0",
    "radash": "12.1.1",
    "devalue": "5.1.1",
    "bottleneck": "2.19.5",
    "ts-ag": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^5.28.2",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.2.9",
    "tailwind-variants": "^1.0.0",
    "@lucide/svelte": "^0.508.0"
  },
  "devDependencies": {
    "svelte": "^5.34.7",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@eslint/js": "^9.29.0",
    "@iconify/types": "^2.0.0",
    "@iconify/tailwind4": "^1.0.6",
    "@iconify/json": "^2.2.351",
    "@lucide/svelte": "^0.522.0",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4",
    "@tailwindcss/vite": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "eslint": "^9.29.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-better-tailwindcss": "3.4.0",
    "eslint-plugin-svelte": "^3.9.3",
    "globals": "^16.2.0",
    "prettier": "^3.6.0",
    "prettier-plugin-svelte": "^3.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "typescript-svelte-plugin": "0.3.48",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "npm-check-updates": "^18.0.1",
    "@types/node": "^24.0.3",
    "rollup": "^4.44.0"
  },
  "type": "module",
  "files": [
    "./src",
    "./dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./vite": {
      "types": "./dist/lib/vite/index.d.ts",
      "default": "./dist/lib/vite/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "svelte": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "repository": "ageorgeh/svelte-ag",
  "scripts": {
    "project:package": "svelte-package -i src",
    "project:watch": "svelte-package -i src -w",
    "svelte:sync": "svelte-kit sync",
    "publish:local": "pnpm version prerelease --preid dev --no-git-tag-version && pnpm publish --registry http://localhost:4873 --tag dev --access public --no-git-checks --json > ./publishLocal.json",
    "publish:prerelease": "pnpm publish --tag dev --access public --no-git-checks --registry=https://registry.npmjs.org/ --json > ./publish.json",
    "version:prerelease": "pnpm version prerelease --preid dev --no-git-tag-version"
  }
}
