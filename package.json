{
  "name": "svelte-ag",
  "version": "1.0.9",
  "description": "Useful svelte components",
  "bugs": "https://github.com/ageorgeh/svelte-ag/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ageorgeh/svelte-ag.git"
  },
  "author": "Alexander Hornung",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./vite": {
      "types": "./dist/lib/vite/index.d.ts",
      "default": "./dist/lib/vite/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "svelte": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "files": [
    "./src",
    "./dist"
  ],
  "scripts": {
    "prebuild": "svelte-kit sync && svelte-check --fail-on-warnings",
    "build": "svelte-package -i src",
    "dev": "vite dev",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "husky",
    "svelte:sync": "svelte-kit sync",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:update": "playwright test --update-snapshots",
    "watch": "svelte-package -i src -w"
  },
  "dependencies": {
    "@dnd-kit-svelte/svelte": "^0.1.6",
    "@floating-ui/dom": "^1.7.5",
    "bits-ui": "^2.16.2",
    "bottleneck": "^2.19.5",
    "clsx": "^2.1.1",
    "dequal": "^2.0.3",
    "devalue": "^5.6.3",
    "embla-carousel-svelte": "8.6.0",
    "formsnap": "2.0.1",
    "radash": "12.1.1",
    "runed": "0.37.1",
    "svelte-toolbelt": "^0.10.6",
    "sveltekit-superforms": "2.30.0",
    "tailwind-merge": "^3.5.0",
    "tailwind-variants": "^3.2.2",
    "ts-ag": "^1.0.16",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@actions/languageserver": "^0.3.45",
    "@eslint/js": "^10.0.1",
    "@iconify/json": "^2.2.443",
    "@iconify/tailwind4": "^1.2.1",
    "@iconify/types": "^2.0.0",
    "@internationalized/date": "^3.11.0",
    "@lucide/svelte": "^0.575.0",
    "@playwright/test": "1.57.0",
    "@sveltejs/kit": "^2.53.2",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^7.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.2.1",
    "@types/node": "^24.10.14",
    "@typescript/native-preview": "7.0.0-dev.20260225.1",
    "eslint": "^10.0.2",
    "eslint-plugin-better-tailwindcss": "^4.3.0",
    "eslint-plugin-svelte": "^3.15.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-svelte": "^3.5.0",
    "semantic-release": "^25.0.3",
    "svelte": "^5.53.5",
    "svelte-check": "^4.4.3",
    "tailwindcss": "4.2.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "typescript-svelte-plugin": "^0.3.50",
    "vite": "^8.0.0-beta.15",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^5.0.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.2.1",
    "tw-animate-css": "^1.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide"
    ],
    "overrides": {
      "vite": "8.0.0-beta.15"
    }
  }
}
