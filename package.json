{
  "name": "svelte-ag",
  "description": "Useful svelte components",
  "version": "0.0.2-dev.71",
  "author": "Alexander Hornung",
  "bugs": "https://github.com/ageorgeh/cms/issues",
  "dependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "svelte": "^5.25.2",
    "@iconify/types": "^2.0.0",
    "tw-animate-css": "^1.2.5",
    "mode-watcher": "^0.5.1",
    "bits-ui": "^1.3.15",
    "svelte-toolbelt": "^0.7.1",
    "vitest": "^3.1.1",
    "tailwind-variants": "^1.0.0",
    "tailwind-merge": "^3.2.0",
    "clsx": "^2.1.1"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "files": [
    "./src",
    "./dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*"
    }
  },
  "repository": "ageorgeh/svelte-ag",
  "scripts": {
    "components:package": "svelte-package -i src --tsconfig tsconfig.build.json",
    "components:watch": "svelte-package -i src -w --tsconfig tsconfig.build.json",
    "components:dev": "vite --config vite.config.ts dev",
    "publish:local": "pnpm version prerelease --preid dev --no-git-tag-version && pnpm publish --registry http://localhost:4873 --tag dev --access public --no-git-checks --json > ./publishLocal.json"
  }
}
